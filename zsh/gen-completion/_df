#compdef df

# zsh completions for 'df'
# automatically generated with http://github.com/RobSis/zsh-completion-generator
local arguments

arguments=(
    {-a,--all}'[include pseudo, duplicate, inaccessible file systems]'
    {-B,--block-size}'[scale sizes by SIZE before printing them; e.g.]'
    {-h,--human-readable}'[print sizes in powers of 1024 (e.g., 1023M)]'
    {-H,--si}'[print sizes in powers of 1000 (e.g., 1.1G)]'
    {-i,--inodes}'[lista informações do inode, em vez de uso de bloco]'
    '-k[o mesmo que --block-size=1K]'
    {-l,--local}'[limita a listagem a sistemas locais de arquivo]'
    '--no-sync[não invocar "sync" ao obter informações de uso (padrão)]'
    '--output[use the output format defined by FIELD_LIST]'
    {-P,--portability}'[use the POSIX output format]'
    '--sync[invoke sync before getting usage info]'
    '--total[elide all entries insignificant to available space]'
    {-t,--type}'[limit listing to file systems of type TYPE]'
    {-T,--print-type}'[print file system type]'
    {-x,--exclude-type}'[limit listing to file systems not of type TYPE]'
    '-v[(ignored)]'
    '--help[mostra esta ajuda e finaliza]'
    '--version[informa a versão e finaliza]'
    '*:filename:_files'
)

_arguments -s $arguments
